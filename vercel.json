{
  "functions": {
    "app/api/*": {
      "maxDuration": 60
    },

    "app/api/deleteCollection": {
      "schedule": "58 4 * * *"
    }
    
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        }
      ]
    }
  ]
}